{
  "name": "sports-scheduler",
  "version": "1.0.0",
  "description": "My Sports Scheduling Application",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -e js,ejs",
    "db:create:test": "NODE_ENV=test npx sequelize-cli db:create",
